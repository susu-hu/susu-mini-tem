/* 19 */
.loader19{
  width: 100px;
  height: 100px;
  border-radius: 50%;
  animation: loader8Animation 1.5s linear infinite;
}
.loader19:before {
  width: 20px;
  height: 20px;
  content: "";
  position: absolute;
  background-color: transparent;
  top: 40px;
  left: 40px;
  border-radius: 50%;
  box-shadow: 0px -50px 0px 0px rgba(255, 255, 255, 1),
    -36px -36px 0px -1px rgba(255, 255, 255, 0.875),
    -50px 0px 0px -2px rgba(255, 255, 255, 0.75),
    -36px 36px 0px -3px rgba(255, 255, 255, 0.625),
    0px 50px 0px -4px rgba(255, 255, 255, 0.5),
    36px 36px 0px -5px rgba(255, 255, 255, 0.375),
    50px 0px 0px -6px rgba(255, 255, 255, 0.25),
    36px -36px 0px -7px rgba(255, 255, 255, 0.125);
}
@keyframes loader8Animation {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

/* 20 */
.box2{
  height:400rpx;
}
.loader20 {
  background-color: #0f59c9;
  border-radius: 50%;
  box-shadow: 0 0 30rpx #c94646;
  width: 150rpx;
  height: 150rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  position: absolute;
}

.earth-orbit {
  animation: orbit 6s linear infinite;
  height: 300rpx;
  width: 300rpx;
  left: 50%;
  margin-left: -150rpx;
  position: absolute;
  border-radius: 50%;
  border: 1px solid yellow;
  box-sizing: border-box;
  transform: rotate(0deg);
  transform-origin: center;
}

.earth-orbit::after{
  position: absolute;
  content: '';
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
  background: #000;
  top: 30px;
  right: 0;
}

.moon{
  animation: orbit 6s linear infinite;
  height: 100rpx;
  width: 100rpx;
  top: 10px;
  right: -15px;
  position: absolute;
  border-radius: 50%;
  border:1px solid yellow;
  box-sizing: border-box;
  transform: rotate(0deg);
  transform-origin: center;
}

.moon::after{
  position: absolute;
  content: '';
  border-radius: 50%;
  width: 20rpx;
  height: 20rpx;
  background: #000;
  top: 0px;
  right: 4px;
}


@-webkit-keyframes orbit {
  to {
    transform: rotate(360deg);
  }
}

@keyframes orbit {
  to {
    transform: rotate(360deg);
