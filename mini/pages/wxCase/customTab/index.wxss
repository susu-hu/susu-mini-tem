/* pages/wxCase/customTab/index.wxss */

.fix_nav_wp{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
}





.five {
  position: relative;
  width: 100px;
  height: 100px;
  border-radius: 50%;
  border: solid #fff 4px;
}
.five:before {
  content: "";
  height: 42px;
  width: 4px;
  position: absolute;
  left: 50%;
  top: 45%;
  background: greenyellow;
  border-radius: 2px;
  transform-origin: 2px 40px;
  /* animation: loader5Animation 12s linear infinite; */
}
.five:after {
  content: "";
  height: 27px;
  width: 4px;
  position: absolute;
  left: 50%;
  top: 45%;
  background: red;
  border-radius: 2px;
  transform-origin: 2px 25px;
  /* animation: loader5Animation 12s linear infinite; */
}
