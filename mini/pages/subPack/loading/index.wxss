.binary {
  width: 100%;
  height: 140px;
  display: block;
  color: #2D4654;
  position: absolute;
  top: 0;
  left: 15px;
  z-index: 2;
  overflow:hidden;
}
.binary::before, .binary::after {
  font-size: 24rpx;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}

.binary:nth-child(1)::before{
    content: "0";
    animation: a 1.1s linear infinite;
}
.binary:nth-child(1)::after{
  content: "0";
  animation: b 1.3s linear infinite;
}
.binary:nth-child(2)::before {
  content: "1";
  animation: c 0.9s linear infinite;
}
.binary:nth-child(2)::after{
  content: "1";
  animation: d 0.7s linear infinite;
}

@keyframes a {
	0% {
