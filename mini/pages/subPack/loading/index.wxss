/* 13 */
.css-heart{
  position: absolute;
  width: 60px;
  height: 50px;
  animation: heart_sc 1s linear infinite;
}
@keyframes heart_sc {
  0%{
    transform: scale(.5);  /*开始为原始大小*/
  }
  25%{
      transform: scale(.8); /*放大1.1倍*/
  }
  50%{
      transform: scale(.5);
  }
  75%{
      transform: scale(1);
  }
}
.css-heart:before,
.css-heart:after {
    position: absolute;
    content: "";
    left: 30px;
    top: 0;
    width: 30px;
    height: 50px;
    background: #950d0d;
    border-radius: 50px 50px 0 0;
    -webkit-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
    -webkit-transform-origin: 0 100%;
    -ms-transform-origin: 0 100%;
    transform-origin: 0 100%;
}

.css-heart:after {
  left: 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
  -webkit-transform-origin: 100% 100%;
  -ms-transform-origin: 100% 100%;
  transform-origin: 100% 100%;
}
/* 14 */
.circle-spin{
  width: 100px;
  height: 100px;
  box-sizing: border-box;
  border-radius: 50%;
  border-top: 10px solid #8072a1;
  /* border-bottom:2px solid #351979; */
  animation: an14 2s linear infinite;
}
.circle-spin::before,.circle-spin::after{
  position: absolute;
  content: "";
  width: 100px;
  height: 100px;
  left: 0;
  top: -10px;
  box-sizing: border-box;
  border-radius: 50%;
}
.circle-spin::before{
  border-top:10px solid #351979 ;
  transform: rotate(120deg);
}
.circle-spin::after{
  border-top:10px solid #2b155e ;
  transform: rotate(240deg);
}
.circle-spin text{
  position: absolute;
  display: block;
  width: 100px;
  height: 100px;
  line-height:100px;
  text-align: center;
  font-size: 20rpx;
  color: #2b155e;
  animation:  an14-1 2s linear infinite;
}
@keyframes an14 {
	100% {
	 transform:rotateZ(360deg);
	}
}
@keyframes an14-1 {
	100% {
	 transform:rotateZ(-360deg);
	}
}
/* 15 */
.circle-add .a1{
  display: block;
  width: 40px;
  height: 50px;
  border-radius: 50%;
  position: absolute;
  top: calc(50% - 25px);
  left: calc(50% - 40px);
  transform-origin: center right; 
}
.circle-add .a1::before{
  content: '';
  display: block;
  width: 8px;
  height: 8px;
  border-radius: 50%;
  position: absolute;
  top: 0;
  right: 10px;
  background-color: #c77d0f;
  animation: circle-add 3s  ease-in-out infinite; 
}
@keyframes circle-add{
  0%{
    transform: scale(0.2);
  }
  100%{
    transform: scale(1);
  }
}
.a1:nth-child(1){
  transform: rotate(0deg);
}
.a1:nth-child(2){
  transform: rotate(45deg);
}
.a1:nth-child(3){
  transform: rotate(90deg);
}

.a1:nth-child(1)::before{
  animation-delay: .2s;
}
.a1:nth-child(2)::before{
  animation-delay: 0.4s;
}
.a1:nth-child(3)::before{
  animation-delay: 0.6s;
}
.a1:nth-child(4){
  transform: rotate(135deg);
}
.a1:nth-child(4)::before{
  animation-delay: .8s
}
.a1:nth-child(5){
  transform: rotate(180deg);
}
.a1:nth-child(5)::before{
  animation-delay: 1s;
}
.a1:nth-child(6){
  transform: rotate(225deg);
}
.a1:nth-child(6)::before{
  animation-delay: 1.2s;
}
.a1:nth-child(7){
  transform: rotate(270deg);
}
.a1:nth-child(7)::before{
  animation-delay: 1.4s;
}
.a1:nth-child(8){
  transform: rotate(315deg);
}
.a1:nth-child(8)::before{
  animation-delay: 1.6s;
}
